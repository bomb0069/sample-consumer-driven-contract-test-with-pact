{
  "consumer": {
    "name": "InventoryWeb"
  },
  "interactions": [
    {
      "description": "a request for product 10",
      "providerState": "a product with ID 10 exists",
      "request": {
        "method": "GET",
        "path": "/api/products/10"
      },
      "response": {
        "body": {
          "currency": "THB",
          "id": 10,
          "name": "Premium Coffee Beans",
          "price": 450,
          "sku": "A-10"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.5.2"
    },
    "pactRust": {
      "ffi": "0.4.20",
      "models": "1.2.0"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "ProductService"
  }
}