{
  "consumer": {
    "name": "InventoryWebB"
  },
  "interactions": [
    {
      "description": "a request for product 10 availability",
      "providerState": "a product with ID 10 exists",
      "request": {
        "method": "GET",
        "path": "/api/products/10"
      },
      "response": {
        "body": {
          "available": true,
          "id": 10,
          "lastRestockDate": "2025-01-01T00:00:00Z",
          "name": "Premium Coffee Beans",
          "sku": "A-10",
          "stockStatus": "IN_STOCK"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.available": {
            "match": "type"
          },
          "$.body.lastRestockDate": {
            "match": "regex",
            "regex": "^\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d([+-][0-2]\\d:[0-5]\\d|Z)$"
          },
          "$.body.stockStatus": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.5.2"
    },
    "pactRust": {
      "ffi": "0.4.20",
      "models": "1.2.0"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "ProductService"
  }
}